<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsPackable>true</IsPackable>
    <PackageId>dev.reddin.BootstrapHtmlHelpers</PackageId>
    <Version>0.0.17</Version>
    <Authors>Jasper Reddin</Authors>
    <PackageTags>mvc;bootstrap</PackageTags>
    <PackageProjectUrl>https://github.com/DrOverbuild/BootstrapHtmlHelpers/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/DrOverbuild/BootstrapHtmlHelpers/</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <TargetFrameworks>net8.0;net6.0</TargetFrameworks>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <None Include="$(SolutionDir)/README.md" Pack="true" PackagePath="/"/>
  </ItemGroup>
  
</Project>
